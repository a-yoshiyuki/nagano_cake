<h1>会員一覧</h1>
<table class="table table-striped">
	<thead>
    <tr>
      <th></th>
      <th>会員ID</th>
      <th>氏名</th>
      <th>メールアドレス</th>
    </tr>
  </thead>

  <tbody>
  	<% @customers.each do |f| %>
  		<tr>
  			<td></td>
  			<td>
  				<%= f.id %>
  			</td>
  			<td>

  					<%= f.name %>

  			</td>
  			<td>
  				<%= f.email %>
  			</td>
  		</tr>
		<% end %>
		<%= paginate @customers %>
  </tbody>

</table>